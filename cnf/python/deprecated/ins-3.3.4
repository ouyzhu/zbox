ins_src_addr            = http://www.python.org/ftp/python/3.3.4/Python-3.3.4.tar.xz
ins_verify              = "ZBOX_INS_FULLPATH/bin/python3.3 --version 2>&1 | grep '^Python 3.3' &> /dev/null"
ins_post_script         = "cd ZBOX_INS_FULLPATH/bin && ln -s python3.3 python"
                        # --enable-shared is needed when compile wsgi which used by dijango, --enable-unicode=ucs4 seem need by django/sqllite, why?
ins_make_configure_opts = "--prefix=ZBOX_INS_FULLPATH --enable-shared --with-ssl --enable-unicode=ucs4"
